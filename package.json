{
	"name": "web-release-detector",
	"version": "0.1.8",
	"description": "A release version inspector for web applications",
	"main": "./lib/cjs/index.js",
	"module": "./lib/esm/index.js",
	"types": "./lib/types/index.d.ts",
	"files": [
		"lib",
		"src"
	],
	"scripts": {
		"dev": "cross-env NODE_ENV=development rollup -c rollup.config.mjs -w",
		"build": "cross-env NODE_ENV=production rollup -c rollup.config.mjs && npm run build:types",
		"build:types": "tsc -p tsconfig.declaration.json",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"pub": "ts-node --project tsconfig.node.json publish/pub.ts"
	},
	"keywords": [
		"release",
		"inspect",
		"version",
		"update"
	],
	"author": "Developer Team",
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.12.10",
		"@babel/preset-env": "^7.12.11",
		"@rollup/plugin-babel": "^5.2.2",
		"@rollup/plugin-commonjs": "^23.0.2",
		"@rollup/plugin-json": "^5.0.2",
		"@rollup/plugin-node-resolve": "^15.0.1",
		"@rollup/plugin-typescript": "^10.0.1",
		"@types/chalk": "^0.4.31",
		"@types/jest": "^30.0.0",
		"@types/js-cookie": "^3.0.2",
		"@types/jsdom": "^27.0.0",
		"@types/node": "^18.0.0",
		"@types/shelljs": "^0.8.11",
		"@typescript-eslint/eslint-plugin": "^5.44.0",
		"@typescript-eslint/parser": "^5.44.0",
		"chalk": "^4.1.2",
		"cross-env": "^7.0.3",
		"eslint": "^8.28.0",
		"jest": "^30.2.0",
		"jest-environment-jsdom": "^30.2.0",
		"jsdom": "^26.1.0",
		"rollup": "^2.37.1",
		"rollup-plugin-serve": "^1.1.0",
		"shelljs": "^0.8.5",
		"ts-jest": "^29.4.5",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.3"
	},
	"publishConfig": {
		"access": "public"
	}
}